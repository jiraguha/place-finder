{
  "name": "place-finder",
  "version": "1.0.0",
  "description": "CLI tool to find places using Google Maps API",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "place-finder": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "cli",
    "places",
    "google-maps",
    "geocoding"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.14",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}